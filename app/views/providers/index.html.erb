<%= render 'partials/content_header', back: false,backPath:'' ,title: @title,add:1,text:'<i class="fa fa-plus"></i> Nuevo Provedor',path_action: new_provider_path %>
<section class="content">
  <div class="box">
    <!-- /.box-header -->
    <div class="box-body table-responsive">
      <table class="table table-striped">
        <thead>
        <tr>
          <th>Proveedor</th>
          <th>Fiction name</th>
          <th>Tel√©fono</th>
          <th>Correo</th>
          <th>Cuit</th>
          <th colspan="2"></th>
        </tr>
        </thead>

        <tbody>
        <% @providers.each do |provider| %>
            <tr>
              <td><%= provider.name %></td>
              <td><%= provider.fiction_name %></td>
              <td><%= provider.phone %></td>
              <td><%= provider.email %></td>
              <td><%= provider.cuit %></td>
              <td><%= link_to '', provider , class: 'fa fa-eye'%></td>
              <td><%= link_to '', edit_provider_path(provider), class: 'fa fa-pencil' %></td>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</section>

